Algoritmo "exe006"

Var
   palavra1, palavra2, palavra3: caracter
Inicio
      escreval("Digite uma palavra: ")
      leia(palavra1)
      escreval("Digite outra palavra: ")
      leia(palavra2)
      palavra3 <- palavra1 + palavra2
      escreval("As duas palavras juntas: ", palavra3)
Fimalgoritmo