programa {
  funcao inicio() {

    // Declaração de Variável Numérica
    inteiro numero

    // Solicitiando um Número e Realizando a Leitura
    escreva("Digite um numero entre 10 e 45: ")
    leia(numero)

    // Realizando a Verificação
    se ((numero >= 10) e (numero <= 45)){
      escreva("Numero valido")
    } senao {
      escreva("Numero invalido")
    }
    
  }
}
