Algoritmo "exe011"

Var
   nota1, nota2, nota3: inteiro
   media: real
Inicio
      escreval("Digite a primeira nota: ")
      leia(nota1)
      escreval("Digite a segunda nota: ")
      leia(nota2)
      escreval("Digite a terceira nota: ")
      leia(nota3)
      media <- (nota1 + nota2 + nota3) / 3
      escreval("Media: ", media)
Fimalgoritmo