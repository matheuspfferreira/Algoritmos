Algoritmo "exe014"

Var
   num1, num2: inteiro
   valorFinal: real
Inicio
      escreva("Digite o primeiro numero: ")
      leia(num1)
      escreva("Digite o segundo numero: ")
      leia(num2)
      valorFinal <- (num1 - num2) * 0.1
      escreval("Valor final: ", valorFinal)
Fimalgoritmo