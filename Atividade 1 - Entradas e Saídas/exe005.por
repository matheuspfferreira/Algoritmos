programa {
  funcao inicio() {

    // Declaração de Variáveis e Constantes
    const real pi = 3.14
    real raio, comprimento

    // Entradas e Saídas
    escreva("Digite o valor do raio: ")
    leia(raio)

    comprimento = 2 * pi * raio
    escreva("O comprimento do circulo mede: ", comprimento)
    
  }
}