Algoritmo "exe013"

Var
   num1, num2, num3, valorFinal: inteiro
Inicio
      escreva("Digite o primeiro numero: ")
      leia(num1)
      escreva("Digite o segundo numero: ")
      leia(num2)
      escreva("Digite o terceiro numero: ")
      leia(num3)
      valorFinal <- (num1 + num3) * num2
      escreval("Valor final: ", valorFinal)
Fimalgoritmo