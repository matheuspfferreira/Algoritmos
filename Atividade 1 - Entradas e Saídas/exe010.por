programa {
  funcao inicio() {

    // Declaração de Variáveis e Constantes
    const real pi = 3.14
    real raio, volume

    // Entradas e Saídas
    escreva("Digite o valor do raio: ")
    leia(raio)
    
    volume = 1.33 * pi * raio * raio * raio
    escreva("O volume da esfera mede: ", volume)

  }
}